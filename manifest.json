{
  "name": "NewTab Opener for Kibela",
  "short_name": "Kibela Opener",
  "description": "open links in a kibela's article in a new tab",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.kibe.la/*"
        
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
